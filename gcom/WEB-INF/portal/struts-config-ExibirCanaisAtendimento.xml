<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<global-forwards />
	
	<action-mappings>
		<action name="ExibirCanaisAtendimentoActionForm"
		    type="gcom.gui.portal.ExibirCanaisAtendimentoAction"
			validate="false" 
			scope="session" 
			path="/canais-de-atendimento">
			
			<forward name="callcenter" path="/jsp/portal/callcenter.jsp" />
			<forward name="ouvidoria" path="/jsp/portal/ouvidoria.jsp" />

		</action>
	</action-mappings>

	<message-resources null="false" parameter="gcom.properties.application" />
</struts-config>

